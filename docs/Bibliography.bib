issn = {1932-6203},
journal = {PLoS ONE},
number = {3},
pages = {e0118261},
pmid = {25739030},
title = {{Quantitative computed tomographic descriptors associate tumor shape complexity and intratumor heterogeneity with prognosis in lung adenocarcinoma.}},
