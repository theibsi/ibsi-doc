file = {:C$\backslash$:/Users/zwanenbal/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mackin et al. - 2015 - Measuring Computed Tomography Scanner Variability of Radiomics Features.pdf:pdf},
isbn = {0000000000000},
