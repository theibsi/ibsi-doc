number = {12},
pages = {749--762},
pmid = {28975929},
title = {{Radiomics: the bridge between medical imaging and personalized medicine.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28975929},
volume = {14},
year = {2017}
}
